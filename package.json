{
  "name": "rxblox-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter rxblox-demo dev",
    "dev:todo": "concurrently \"pnpm --filter rxblox build:watch\" \"pnpm --filter rxblox-todo dev\"",
    "build": "pnpm --filter rxblox build",
    "build:rextive": "pnpm --filter rextive build",
    "build:all": "pnpm --filter rxblox build && pnpm --filter rextive build",
    "test": "pnpm --filter rxblox test",
    "test:rextive": "pnpm --filter rextive test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf",
  "pnpm": {
    "overrides": {
      "lodash.get": "npm:lodash@^4.17.21",
      "lodash.isequal": "npm:lodash@^4.17.21"
    }
  }
}
